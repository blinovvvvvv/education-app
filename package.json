{
	"name": "client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"dev": "node server",
		"build": "rm -rf ./dist && yarn build:client && yarn build:server",
		"build:client": "vite build --outDir dist/client",
		"build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
		"generate": "rm -rf ./dist && vite build --outDir dist/static && npm run build:server && node prerender",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && stylelint \"./src/**/*.scss\"",
		"serve": "cross-env NODE_ENV=production node server",
		"test": "vitest --config ./vitest.config.ts",
		"test:ui": "vitest --ui --config ./vitest.config.ts"
	},
	"dependencies": {
		"clsx": "^1.2.1",
		"cross-env": "^7.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet-async": "^1.3.0",
		"react-router-dom": "^6.11.1"
	},
	"devDependencies": {
		"@feature-sliced/eslint-config": "^0.1.0-beta.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.4.3",
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.17",
		"@types/node": "^20.1.0",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"@vitejs/plugin-legacy": "^4.0.3",
		"@vitejs/plugin-react": "^4.0.0",
		"@vitest/ui": "^0.31.1",
		"autoprefixer": "^10.4.14",
		"compression": "^1.7.4",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-boundaries": "^3.1.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.3.4",
		"express": "^4.18.2",
		"husky": "^8.0.3",
		"jsdom": "^22.0.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"sass": "^1.62.1",
		"serve-static": "^1.15.0",
		"stylelint": "^15.6.1",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-config-standard-scss": "^9.0.0",
		"stylelint-prettier": "^3.0.0",
		"tailwindcss": "^3.3.2",
		"typescript": "^5.0.2",
		"vite": "^4.3.2",
		"vite-plugin-checker": "^0.6.0",
		"vite-plugin-pwa": "^0.14.7",
		"vitest": "^0.31.0",
		"workbox-window": "^6.5.4"
	}
}
